{
"app"
:
{
"#desc"
:
{
"en"
:
"Interactive UI show workflow runs"
,
"cn"
:
"交互式表格显示工作流运行信息"
}
,
"--repo|-r"
:
{
"#synopsis"
:
"<owner/repo>"
,
"#desc"
:
{
"en"
:
"<ownerPath>/<RepoPath> (default ownerPath: current user)"
,
"cn"
:
"<所属空间地址>/<仓库地址> (默认所属空间地址: 当前用户空间地址)"
}
}
,
"--branch"
:
{
"#synopsis"
:
"<branch>"
,
"#desc"
:
{
"en"
:
"Returns workflow runs associated with a branch"
,
"cn"
:
"返回与分支关联的工作流运行"
}
}
,
"--actor"
:
{
"#synopsis"
:
"<username>"
,
"#desc"
:
{
"en"
:
"returns someone workflow runs"
,
"cn"
:
"返回目标用户的工作流运行"
}
}
,
"--event"
:
{
"#synopsis"
:
"<event>"
,
"#desc"
:
{
"en"
:
"Returns workflow run triggered by the event you specify"
,
"cn"
:
"返回指定的事件触发的工作流运行"
}
}
,
"--sta|--status"
:
{
"#synopsis"
:
"<status>"
,
"#desc"
:
{
"en"
:
"Returns workflow runs with the check run status"
,
"cn"
:
"返回指定运行状态的工作流运行"
}
}
,
"--created"
:
{
"#synopsis"
:
"<ISO 8601>"
,
"#desc"
:
{
"en"
:
"Returns workflow runs created within the given date-time range"
,
"cn"
:
"返回日期时间范围内创建的工作流运行"
}
}
,
"--check_suite_id"
:
{
"#synopsis"
:
"<id>"
,
"#desc"
:
{
"en"
:
"Returns workflow runs with the check_suite_id that you specify"
,
"cn"
:
"使用指定的 check_suite_id 工作流运行"
}
}
,
"--page"
:
{
"#desc"
:
{
"en"
:
"target page"
,
"cn"
:
"目标页码"
}
}
,
"--per_page"
:
{
"#desc"
:
{
"en"
:
"per_page data number"
,
"cn"
:
"每页数据量"
}
}
,
"#option:required"
:
[
"--repo|-r"
]
,
"#option:optional"
:
[
"--branch"
,
"--actor"
,
"--event"
,
"--sta|--status"
,
"--created"
,
"--check_suite_id"
,
"--page"
,
"--per_page"
]
,
"--exclude_pull_requests"
:
{
"#desc"
:
{
"en"
:
"Not pull requests are omitted from the response"
,
"cn"
:
"返回中省略 pull requests"
}
}
}
