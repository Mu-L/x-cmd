"download|install"
:
{
"#desc"
:
{
"en"
:
"Download artifact from repo artifacts"
,
"cn"
:
"下载仓库的构建产物"
}
,
"#tldr"
:
[
{
"cmd"
:
"x gh action artifact download --repo <owner/repo> <artifact_id>"
,
"en"
:
"Download <artifact_id> artifact from <owner/repo> repo artifacts to local default path"
,
"cn"
:
"从 <owner/repo> 仓库中 下载 <artifact_id> 构建产物"
}
,
{
"cmd"
:
"x gh action artifact download --repo <owner/repo> --output . <artifact_id>"
,
"en"
:
"Download <artifact_id> artifact from <owner/repo> repo artifacts to local current path"
,
"cn"
:
"从 <owner/repo> 仓库中 下载 <artifact_id> 构建产物到当前路径中"
}
]
