# shellcheck shell=dash

___x_cmd_brew___xmirror(){
    [ $# -gt 0 ]    ||  set - --help
    local op="$1";      shift
    case "$op" in
        ls|set|unset|current)   x mirror brew "$op" "$@" ;;  # In the future, we will put the mirror code here ...

        -h|--help)      x help -m brew mirror "$@" ;;
        # *)              ___x_cmd_brew_mirror___exec "$op" "$@" ;;
    esac
}
