# shellcheck    shell=sh        disable=SC2039,SC1090,SC3043,SC2263

# license:      GPLv3

xrc:mod:lib     open            main

___x_cmd_open(){
    case "$1" in
        -h|--help)          x help -m open 1>&2
                            return 1 ;;
                            
        *)   ___x_cmd_open___main "$@"
    esac
}

xrc setmain ___x_cmd_open
