# shellcheck shell=dash

xrc:mod:lib     onsh        __setup ddgo

___x_cmd_onsh___main(){
    [ "$#" -gt 0 ] ||       set - --help

    local op="$1"; shift
    case "$op" in
        setup|--setup)      ___x_cmd_onsh___setup   "$@" ;;
        :|ddgo)             ___x_cmd_onsh_ddgo      "$@" ;;
        *)                  ___x_cmd_onsh___cmd     "$@" ;;
    esac
}

___x_cmd_onsh___cmd(){
    ___x_cmd_hascmd xonsh || {
        x env try xonsh || return
    }

    ___x_cmd_cmds xonsh "$@"
}
