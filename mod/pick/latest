# shellcheck    shell=sh        disable=SC2039,SC1090,SC3043,SC2263

# license:      GPLv3

xrc:mod:lib     pick    main

___x_cmd_pick(){
    case "$1" in
        --help|-h)  ___x_cmd_pick_help ;;
        *)          ___x_cmd_pick___main "$@"
    esac
}

___x_cmd_pick_help(){
    x help -m pick 1>&2
    return 1
}

xrc setmain ___x_cmd_pick
