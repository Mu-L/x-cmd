

___X_CMD_ROOT="${___X_CMD_ROOT:-$HOME/.x-cmd.root}"

___x_cmd_x_bin_xsh(){
    case "$0" in
        */mod/x-cmd/lib/bin/xsh)
            ___X_CMD_ROOT_CODE="${0%/mod/x-cmd/lib/bin/xsh}"
            ;;
        *)
            return 1
            ;;
    esac

    . "$___X_CMD_ROOT_CODE/X"
}

___x_cmd_x_bin_xsh || exit 1

x "$@"
