
___x_cmd_mac_net(){
    [ "$#" -gt 0 ] || set -- -h

    local op="$1";  shift
    case "$op" in
        test|port|ip)
            ___x_cmd_mac_net_"$op" "$@"
    esac
}

___x_cmd_mac_net_test(){
    command networkQuality -v
}

___x_cmd_mac_net_port(){
    # require sudo ?
    command lsof -iTCP -sTCP:LISTEN -P
}

___x_cmd_mac_net_ip(){
    x ip
    # ipconfig getifaddr en0
    # ipconfig getifaddr en1
    #
}
