# shellcheck shell=dash

x log init gemini
xrc:mod:lib     gemini      cfg  request/_index chat/_index count/_index request/count
___x_cmd_gemini___main(){
    [ "$#" -gt 0 ] || {

        return
    }

    local op="$1"; shift
    case "$op" in
        --cfg|--cur|--init) ___x_cmd_gemini_"${op#--}" "$@"   ;;
        --request)          ___x_cmd_gemini_request "$@"      ;;
        chat)               ___x_cmd_gemini_chat "$@"         ;;
        -h|--help)          x help -m gemini "$@"             ;;
        *)                  N=gemini M="Unknown subcmd ==> $op" log:ret:64 ;;
    esac
}
