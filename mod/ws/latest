# Author:       Li Junhao           l@x-cmd.com     # xrc
# shellcheck    shell=sh

x log init ws
xrc:mod:lib     ws          util default/_ exec execfile


___x_cmd_ws(){
    case "$1" in

        --help|-h|"")  shift;  x help -m ws >&2; return 1 ;;
        exec)          shift;  ___x_cmd_ws_exec "$@"       ;;
        execfile)      shift;  ___x_cmd_ws_execfile "$@"   ;;
        *)                     ___x_cmd_ws_exec "$@"       ;;
    esac
}

xrc setmain ___x_cmd_ws
