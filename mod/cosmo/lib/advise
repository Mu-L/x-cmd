___x_cmd_cosmo___advise(){
    local IFS="$___X_CMD_UNSEENCHAR_NEWLINE"
    ___x_cmd_pkg_update___check || return
    candidate_exec_arr=($(
        x ja 'D==1{print k()}'  < "$___X_CMD_COSMO_METADATA_PATH/normal/X.json"
    ))
}
