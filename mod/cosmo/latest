xrc:mod:lib     cosmo   main

___x_cmd_cosmo(){
    ___x_cmd_cosmo___main "$@"
}

[ -d "$___X_CMD_ROOT_DATA/cosmo/bin" ] || x mkdirp -p "$___X_CMD_ROOT_DATA/cosmo/bin"
xrc setmain ___x_cmd_cosmo
