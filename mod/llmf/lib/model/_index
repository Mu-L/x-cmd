# shellcheck shell=sh disable=SC3043
xrc:mod:lib     llmf    model/ls    model/nv    model/which \
                        model/info  model/download

___x_cmd_llmf_model(){
    [ "$#" -gt 0 ]      ||   set - nv

    local op="$1";      shift
    case "$op" in
        # pick)           ___x_cmd_llmf_model_pick       "$@" ;;
        default)        ___x_cmd_llmf_model_default    "$@" ;;

        ls)             ___x_cmd_llmf_model_ls "$@" ;;
        nv)             ___x_cmd_llmf_model_nv "$@" ;;
        info)           ___x_cmd_llmf_model_info "$@" ;;
        which)          ___x_cmd_llmf_model_which "$@" ;;
        download)       ___x_cmd_llmf_model_download "$@" ;;
        -h|--help)      x help -m llmf model ;;
        *)              x help -m llmf model >&2; return 1 ;;
    esac
}

___x_cmd_llmf_model_default(){
    local ___X_CMD_VAL_O=llmf/model
    local op="$1"
    case "$op" in
        set|get_|get)
                shift; ___x_cmd_val_default "$op" "$@" ;;
        *)      N=llmf M="Not such option=$op" log:ret:64 ;;
    esac
}
