# shellcheck shell=sh disable=SC3043

___x_cmd_llmf_model_info(){
    local model_name="${1}"
    local model_version="${2}"
    local model_quant="${3}"
    local model_dir="${___X_CMD_ROOT_DATA}"/llmf/model
    local model_path="${model_dir}/data/${model_name}.json"
    [ -f "${model_path}" ] ||  N=llmf M="The model does not exist" log:ret:1
    x jo color < "$model_path"
}
