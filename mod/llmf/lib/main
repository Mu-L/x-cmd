# shellcheck shell=dash
x log init llmf
___x_cmd_llmf___main(){
    [ "$#" -gt 0 ]      ||   set - --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      x help -m llmf; return 1    ;;
        model)          ___x_cmd_llmf_model "$@" ;;

        serve|server)   ___x_cmd_llmf_serve "$@" ;;
        cli|client)     ___x_cmd_llmf_cli   "$@" ;;

        --)             ___x_cmd_llmf_cli "$@" ;;
        *)              ___x_cmd_llmf_cli "$op" "$@" ;;
    esac
}

___x_cmd_llmf___llamafile_exec(){
    [ "$#" -gt 0 ]      ||   set - --help
    x cosmo llamafile "$@"
}

