# shellcheck shell=dash

xrc:mod:lib     co      exec

# co is for copilot
___x_cmd_co___main(){
    [ "$#" -gt 0 ] || set -- -h

    local op="$1"; shift
    case "$op" in
        --help|-h)      ___x_cmd_co___help ;;
        --exec)         ___x_cmd_co_exec "$@" ;;

        # --cmd)          ___x_cmd_co_cmd "$@" ;;
        # convert)        ___x_cmd_co_convert ;;
    esac
}

___x_cmd_co___help(){
    x help -m co
}
