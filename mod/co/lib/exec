# shellcheck shell=dash
___x_cmd_co_exec(){
    local LOCALSHELL=
    local OS=
    
    local x_=""
    x os name_

    LOCALSHELL="$___X_CMD_SHELL" OS="$x_" x chat --request --type "co" --minion "$___X_CMD_ROOT_MOD/co/lib/minion/exec.yml" "$@"
}

# SHELL="$___X_CMD_SHELL" OSNAME="$___X_CMD_OSNAME" x chat --request --minion co.minion.yml --with-type-prompt "co" "$prompt" "$@"




