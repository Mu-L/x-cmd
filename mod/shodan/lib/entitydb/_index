# shellcheck shell=dash
xrc:mod:lib shodan      entitydb/ls entitydb/search

___x_cmd_shodan_entitydb(){
    [ $# -gt 0 ]    || set -- ls

    local op="$1";      shift
    case "$op" in
        ls|search)      ___x_cmd_shodan_entitydb_"$op" ;;
        -h|--help)      x help -m shodan entitydb ;;
    esac
}
