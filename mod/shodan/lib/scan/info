# shellcheck shell=bash
___x_cmd_shodan_scan_info(){
    [ -n "$1" ] || N=shodan M="Provide a scan ID" log:ret:64

    local id="$1"

    if  ! ___x_cmd_shodan_is_interactive_env; then
        ___x_cmd_shodan_scan_info___curl
        return
    fi

    ___x_cmd_shodan_scan_info___curl | x j2y
}

___x_cmd_shodan_scan_info___curl(){
    ___x_cmd_shodan_curl get "/shodan/scan/${id}"
}

