# shellcheck shell=bash
# /account/profile
___x_cmd_shodan_profile(){
     if  ! ___x_cmd_shodan_is_interactive_env; then
        ___x_cmd_shodan_profile___curl
        return
    fi

    ___x_cmd_shodan_profile___curl  | x j2y
}

___x_cmd_shodan_profile___curl(){
    ___x_cmd_shodan_curl get /account/profile
}
