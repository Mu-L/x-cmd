# shellcheck shell=dash
# https://developer.shodan.io/api
___x_cmd_shodan_host_facet(){
    local X_help_cmd='x help -m shodan host facet';   help:arg:parse

    if  ___x_cmd_shodan_is_interactive_env; then ___x_cmd_shodan_host_facet___curl  | x y2j
    else                                         ___x_cmd_shodan_host_facet___curl
    fi
}

___x_cmd_shodan_host_facet___curl(){
     ___x_cmd_shodan_curl get "/shodan/host/search/facets?"
}

