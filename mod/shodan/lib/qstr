# shellcheck shell=dash
___x_cmd_shodan_qstr_(){
    x_=
    local op="$1";      shift
    case "$op" in
        # --hostname|--port|--country|--city|--org|--isp|--product|--version|--geo|--before|--after|--net)
        --*)
            x_="${op#--}: \"$2\""
            ;;
        *)
            N=shodan M="Unknown [option=$op]" log:ret:64 ;;
    esac
}
