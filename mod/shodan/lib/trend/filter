# shellcheck shell=bash
___x_cmd_shodan_trend_search_filter(){
    if ! ___x_cmd_shodan_is_interactive_env; then
        ___x_cmd_shodan_trend_search_filter___curl
        return
    fi

    ___x_cmd_shodan_trend_search_filter___curl | x j2y
}

___x_cmd_shodan_trend_search_filter___curl(){
    X_CCMD_TIMEOUT=10m  \
    ___X_CMD_SHODAN_API_URL=https://trends.shodan.io    \
        ___x_cmd_shodan_curl get "/api/v1/search/filters"
}

