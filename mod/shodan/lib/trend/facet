# shellcheck shell=bash
___x_cmd_shodan_trend_search_facet(){
    if ! ___x_cmd_shodan_is_interactive_env; then
        ___x_cmd_shodan_trend_search_facet___curl
        return
    fi

    ___x_cmd_shodan_trend_search_facet___curl | x j2y
}


___x_cmd_shodan_trend_search_facet___curl(){
    X_CCMD_TIMEOUT=10m  \
    ___X_CMD_SHODAN_API_URL=https://trends.shodan.io    \
        ___x_cmd_shodan_curl get "/api/v1/search/facets"
}

