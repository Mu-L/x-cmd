# shellcheck shell=bash

___x_cmd_shodan_query_search(){
    if  ! ___x_cmd_shodan_is_interactive_env; then
        ___x_cmd_shodan_query_search___curl "$@"
        return
    fi

    ___x_cmd_shodan_query_search___curl "$@" | x j2y
}

___x_cmd_shodan_query_search___curl(){
    local IFS=" "
    ___x_cmd_shodan_ourl get /shodan/query/search --data-urlencode "query=$*"
}
