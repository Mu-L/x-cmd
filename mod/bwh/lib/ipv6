# shellcheck    shell=dash
___x_cmd_bwh_ipv6(){
    case "$1" in
        add|delete)
            local op="$1"; shift
            "___x_cmd_bwh_ipv6_${op}" "$@"
            ;;
    esac
}

___x_cmd_bwh_ipv6_add(){
    [ $# -eq 1 ] || N=bwh M="Require argument as ip" log:ret:64
    local ip="${1}"
    ___x_cmd_bwh_api ipv6/add ip="$ip"
}

___x_cmd_bwh_ipv6_delete(){
    [ $# -eq 1 ] || N=bwh M="Require argument as ip" log:ret:64
    local ip="${1}"
    ___x_cmd_bwh_api ipv6/delete ip="$ip"
}

