# shellcheck    shell=dash
___x_cmd_bwh_info(){
    [ "$#" -gt 0 ] || {
        ___x_cmd_bwh_getLiveServiceInfo
        return 0
    }

    case "$1" in
        get)            ___x_cmd_bwh_getserviceinfo ;;
        live)           ___x_cmd_bwh_getLiveServiceInfo     ;;
        availableos)    ___x_cmd_bwh_getAvailableOS         ;;
        auditlog)       ___x_cmd_bwh_getAuditLog            ;;
        usagegraphs)    ___x_cmd_bwh_getUsageGraphs         ;;
        ratelimit)      ___x_cmd_bwh_getRateLimitStatus     ;;
        rawusage)       ___x_cmd_bwh_getRawUsageStats       ;;
    esac
}

___x_cmd_bwh_getserviceinfo(){
    ___x_cmd_bwh_api getServiceInfo
}

___x_cmd_bwh_getLiveServiceInfo(){
    ___x_cmd_bwh_api getLiveServiceInfo
}

___x_cmd_bwh_getAvailableOS(){
    ___x_cmd_bwh_api getAvailableOS
}


___x_cmd_bwh_getUsageGraphs(){
    ___x_cmd_bwh_api getUsageGraphs
}

___x_cmd_bwh_getAuditLog(){
    ___x_cmd_bwh_api getAuditLog
}


___x_cmd_bwh_getRateLimitStatus(){
    ___x_cmd_bwh_api getRateLimitStatus
}

# getRawUsageStats, getAuditLog, setHostname, setPTR, cloneFromExternalServer, getSuspensionDetails, getPolicyViolations, unsuspend, resolvePolicyViolation, getRateLimitStatus

___x_cmd_bwh_getRawUsageStats(){
    ___x_cmd_bwh_api getRawUsageStats
}

___x_cmd_bwh_getSuspensionDetails(){
    ___x_cmd_bwh_api getSuspensionDetails
}

___x_cmd_bwh_getPolicyViolations(){
    ___x_cmd_bwh_api getPolicyViolations
}

___x_cmd_bwh_resolvePolicyViolation(){
    [ $# -eq 1 ] || N=bwh M="Require argument as record id" log:ret:64
    ___x_cmd_bwh_api resolvePolicyViolation record_id="$1"
}




