# Author:       Li Junhao           l@x-cmd.com     # xrc
# shellcheck    shell=dash

x log init tui

xrc:mod:lib tui     app bigapp

___x_cmd_tui(){
    local op="$1"; shift
    case "$op" in
        bigapp)             ___x_cmd_tui_bigapp "$@"    ;;
        app)                ___x_cmd_tui_app "$@"       ;;
        *)                  ___x_cmd_tui_exec "$@"      ;;
    esac
}

xrc setmain ___x_cmd_tui
