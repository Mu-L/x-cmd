# shellcheck shell=dash
___x_cmd log init find

xrc:mod:lib     find        fzf/_index

___x_cmd_find___main(){
    [ "$#" -gt 0 ] ||   set - --fzfapp

    local op="$1";      shift
    case "$op" in
        --fzfapppreview|--fzfapp)
                    ___x_cmd_find___"${op#--}"  "$@" ;;

        --cmd)      ___x_cmd_find___cmd         "$@" ;;
        *)          ___x_cmd_find___cmd "$op"   "$@" ;;
    esac
}

___x_cmd_find___cmd(){
    ___x_cmd_cmds find "$@"
}
