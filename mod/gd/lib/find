# shellcheck shell=dash

___x_cmd_gd_find(){
    local p=${1}
    local ans
    ans="$(find "$HOME" \
        -path "$HOME/Library" -prune \
        -o -path "$HOME/.*" -prune \
        -o -path "*/.git" -prune \
        -o -name "*$p*" -type d -print 2>/dev/null)"
    if [ "$ans" = "" ]; then
        printf "%s\n" "not found: " 2>/dev/null
        return 1
    fi
    ___x_cmd_gd_choose "$ans"
}
