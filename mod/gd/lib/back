# shellcheck shell=dash

___x_cmd_gd_back_inner(){
    local p; p="${PWD%/*}"
    local target="$1"
    {
        while [ -n "$p" ]; do
            command find "$p" -maxdepth 1 -name "*${target}*" -type d -print
            p="${p%/*}"
        done
        command find / -maxdepth 1 -name "*${target}*" -type d -print
    } | ___x_cmd_gd___util_uniq
}

___x_cmd_gd_back(){
    ___x_cmd_gd_choose ___x_cmd_gd_back_inner "$1"
}
