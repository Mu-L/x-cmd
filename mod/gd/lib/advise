# shellcheck shell=dash

___x_cmd_gd_advise_candidates(){
    local can="$1"
    case "$can" in
        "")
            cat <<A
=
-
...
../
$(ls)
A
        ;;
        ./*)
            ;;
        ../*)
            ;;
        -)  ;;
        --)
            ;;
        -*)
            ;;
        /*) ;;
        */*) ;;


    esac
}
