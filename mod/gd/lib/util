# shellcheck shell=dash

___x_cmd_gd___util_head(){
    command awk -v head="${1:-$___X_CMD_GD_HISTORY_MAXNUM}" '
{
    if ((head!="") && (++c > head)) exit(0)
    print $0
}
'
}

___x_cmd_gd___util_uniq(){
    command awk '
!map[$0]{
    print $0
    map[$0] = 1
}
'
}

___x_cmd_gd___util_reverse(){
    command awk '
{   data[ ++l ] = $0;   }
END{
    for (i=l; i>=1; --i) print data[i];
}
'
}
