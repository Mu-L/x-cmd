___x_cmd_npm___main(){
    [ "$#" -gt 0 ] || {
        ( ___x_cmd_npm___cmd npm )
        return
    }

    ( ___x_cmd_npm___cmd npm "$@" )

    # local op="$1"; shift
    # case "$op" in

    # esac
}

___x_cmd_npm___cmd(){
    x node load || return

    if [ -z "$___X_CMD_NODE_SHIMAPP_PATH" ]; then
        ___x_cmd_cmds "$@"
    else
        local bin="$1"; shift
        "$___X_CMD_NODE_SHIMAPP_PATH/$bin" "$@"
    fi
}

