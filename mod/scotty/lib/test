___x_cmd_scotty_test(){
    param:void

    local yyname
    local yyid

    {
        read -r yyname
        read -r yyid
    } <"${___X_CMD_ROOT_V_VERSION}/mod.sha512.txt"

    git clone git@github.comx:x-cmd/"$yyname"

    (
        cd "$yyname"
        git checkout -b "$yyid" "$yyid"
        cd "mod"
        for arg in *; do
            (
                cd $arg
                xws test
            )
        done
    )
}
