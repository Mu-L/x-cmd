# shellcheck shell=dash
___x_cmd log init rg

xrc:mod:lib     rg      _cmd    fzf/_index

___x_cmd_rg___main(){
    [ "$#" -gt 0 ] ||   set -- --fzfapp
    local op="$1"; shift
    case "$op" in
        --fzfapp|--fzfapppreview)       ___x_cmd_rg___"${op#--}"    "$@" ;;
        --cmd)                          ___x_cmd_rg___cmd           "$@" ;;
        *)                              ___x_cmd_rg___cmd "$op"     "$@" ;;
    esac
}

