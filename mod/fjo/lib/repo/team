    --page          "Page number of the results to fetch."                              <>:Numbers="1"

    --json|-j       "output json data"
A
    param:run
    ___x_cmd_fjo_param_init_owner_repo
    # ___tea_ui_mutual '___ui_table_json Name=.name Slug=.slug Description=.description Privacy=.privacy Permission=.permission -- $@' \
    #     ___x_cmd_fjo_get_multi "/repos/${owner_repo}/teams" visibility affiliation sort direction
    ___x_cmd_fjo_get_multi "/repos/${owner_repo}/teams" visibility affiliation sort direction
}
