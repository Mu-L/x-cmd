# shellcheck shell=dash

x log init openai
xrc:mod:lib     openai      cfg request

___x_cmd_openai___main(){
    X_help_cmd='x help -m openai' help:arg-null:parse

    local op="$1"; shift
    case "$op" in
        --cfg|--cur)    ___x_cmd_openai_"${op#--}" "$@"        ;;
        --request)      ___x_cmd_openai_request "$@"    ;;

        transcribe|image|finetuning|embedding)
                        "___x_cmd_openai_${op}" "$@"
                        ;;
    esac
}


