# shellcheck shell=dash disable=SC2034

x log init openai
xrc param
xrc:mod:lib     openai      cfg request handler audio image embedding finetuning

___x_cmd_openai___main(){
    local X_help_cmd=; X_help_cmd='x help -m openai' help:arg-null:parse

    local op="$1"; shift
    case "$op" in
        --cfg|--cur|--init) ___x_cmd_openai_"${op#--}" "$@" ;;
        --request)          ___x_cmd_openai_request "$@"    ;;
        handler)            ___x_cmd_openai_handler "$@"    ;;

        transcribe|image|finetuning|embedding|audio)
                        "___x_cmd_openai_${op}" "$@"
                        ;;
    esac
}


