# shellcheck shell=dash

xrc:mod:lib     openai      cfg reqeuest

___x_cmd_openai___main(){
    [ "$#" -gt 0 ] || N=openai help:ret:1

    local op="$1"; shift
    case "$op" in
        --cfg)          ___x_cmd_openai_cfg "$@"        ;;
        --request)      ___x_cmd_openai_request "$@"    ;;

        transcribe|image|finetuning|embedding)
                        "___x_cmd_openai_${op}" "$@"
                        ;;
    esac
}


