# shellcheck shell=sh disable=SC3043,2154
xrc:mod:lib     whisper     dictate/main dictate/util

___x_cmd_whisper___dictate(){
    local op="$1";shift
    case "$op" in
        -h|--help)              x help -m whisper dictate                           ;;
        *)                      ___x_cmd_whisper___dictate___main "$op"     "$@"    ;;
    esac
}

