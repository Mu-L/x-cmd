# shellcheck shell=dash

x log init whisper

xrc:mod:lib     whisper     dictate model/_index __convert

___x_cmd_whisper___main(){
    [ "$#" -gt 0  ] ||  set -- model
    local op="$1"; shift
    case "$op" in
        -h|--help)      x help -m whisper "$@" ;;
        dictate)        ___x_cmd_whisper___dictate          "$@" ;;
        model)          ___x_cmd_whisper_model              "$@" ;;
        *)              ___x_cmd_whisper___dictate "$op"    "$@" ;;
    esac
}
