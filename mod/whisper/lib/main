# shellcheck shell=dash
___x_cmd_whisper___main(){
    [ "$#" -gt 0  ] || {
        x help -m whisper
        return 1
    }
    local op="$1"; shift
    case "$op" in
        -h|--help)
            x help -m whisper "$@" ;;
        dictate)
            ___x_cmd_whisper___dictate "$@" ;;
        model)
            ___x_cmd_whisper___model "$@" ;;
        *)
            ___x_cmd_whisper___dictate "$op" "$@" ;;
    esac
}
