# shellcheck shell=dash

___x_cmd_history_last(){
    local x_=
    ___x_cmd_history_last_ || return
    printf "%s\n" "$x_"
}

___x_cmd_history_last_(){
    x_="$(fc -l -1 2>/dev/null)"
    local t=""
    while [ -n "$x_" ]; do
        t="${x_# }"
        [ "$t" != "$x_" ] || break
        x_="$t"
    done
    [ -n "$x_" ]
}

