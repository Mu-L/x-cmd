# shellcheck shell=dash
___x_cmd_apt___xproxy(){
    [ $# -gt 0 ]    ||  set - ls
    local op="$1";      shift

    local O=apt
    case "$op" in
        ls|use|unse|try|untry)
            ___x_cmd proxy wrap "$op"  "$@" ;;

        -h|--help)
            x help -m apt proxy        "$@" ;;
    esac
}


