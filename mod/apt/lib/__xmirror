# shellcheck shell=dash
___x_cmd_apt___xmirror(){
    [ $# -gt 0 ]    ||  set - ls
    local op="$1";      shift

    local O=apt
    case "$op" in
        ls|set|unset|current)
            ___x_cmd mirror wrap "$op"  "$@" ;;

        -h|--help)
            x help -m apt mirror        "$@" ;;
    esac
}


