# shellcheck shell=dash
___x_cmd_dnf___xmirror(){
    [ $# -gt 0 ]    ||  set - --help
    local op="$1";      shift

    local O=dnf
    case "$op" in
        ls|set|rollback|current)   x mirror dnf               "$op" "$@" ;;
        -h|--help)                 x help -m dnf mirror             "$@" ;;
    esac
}
