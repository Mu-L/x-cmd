
___x_cmd_docker_image(){
    local op="$1"; shift
    case "$op" in
        app|"")     ___x_cmd_docker_image_app "$@" ;;
        # ls)         ___x_cmd_docker_iamge_ls "$@" ;;
        *)          ___x_cmd___docker_origin image "$op" "$@"
    esac
}

___x_cmd_docker_image_app(){
    ___x_cmd___docker_origin images "$@"
}

___x_cmd_docker_iamge_ls(){
    ___x_cmd_docker_images_main "$@"
}
