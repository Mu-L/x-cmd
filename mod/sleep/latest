xrc:mod:lib     sleep    main

___x_cmd_sleep(){
    case "$1" in
        -h|--help)
            x advise gen_help "$___X_CMD_ROOT_MOD/sleep/res/advise.jso" 1>&2
            return 1 ;;
        *)                             ___x_cmd_sleep___main "$@"
    esac
}

xrc setmain ___x_cmd_sleep
