
case "$(x os name)" in
    darwin)
___x_cmd_ls_cpu(){
    ls:info "Using sysctl command"
    command sysctl -a | command grep machdep\.cpu | ___x_cmd_cmds_awk -v FS=. '{ split($3, arr, ": "); printf("%-25s: %s\n", arr[1], arr[2]); }'
}
    ;;

    *)
___x_cmd_ls_cpu(){
    ls:info "Using lscpu command"
    command lscpu
}

esac
