# shellcheck shell=dash disable=SC3043,2098

___x_cmd_helpapp_app(){
    local ___X_CMD_HELPAPP_APP_LIST='x'"${___X_CMD_UNSEENCHAR_001}$___X_CMD_ADVISE_MAN_CACHE_X_ADVISE_JSO"
    ! ___x_cmd_is_suitable_advise_env || ___X_CMD_HELPAPP_APP_LIST="${___X_CMD_HELPAPP_APP_LIST}
npx${___X_CMD_UNSEENCHAR_001}$(x advise man which x-cmd npx)
pipx${___X_CMD_UNSEENCHAR_001}$(x advise man which x-cmd pipx)"

    ___X_CMD_HELPAPP_APP_LIST="$___X_CMD_HELPAPP_APP_LIST" \
    ___X_CMD_ADVISE_DEV_MOD="$___X_CMD_ADVISE_DEV_MOD" \
    x tui app \
        -v NO_COLOR="$NO_COLOR" \
        -v WEBSRC_REGION="$___X_CMD_WEBSRC_REGION" \
        -v ___X_CMD_ROOT_ADV="$___X_CMD_ROOT_ADV"  \
        -f "$___X_CMD_ROOT_MOD/awk/lib/j/json.awk"  \
        -f "$___X_CMD_ROOT_MOD/awk/lib/j/jiter.awk" \
        -f "$___X_CMD_ROOT_MOD/awk/lib/j/jop.awk"   \
        -f "$___X_CMD_ROOT_MOD/awk/lib/j/jcp.awk"   \
        -f "$___X_CMD_ROOT_MOD/advise/lib/awk/advise.obj.awk"                   \
        -f "$___X_CMD_ROOT_MOD/advise/lib/awk/advise.share.awk"                 \
        -f "$___X_CMD_ROOT_MOD/advise/lib/awk/advise/advise.util.awk"           \
        -f "$___X_CMD_ROOT_MOD/advise/lib/awk/advise/advise.complete.awk"       \
        -f "$___X_CMD_ROOT_MOD/advise/lib/awk/advise/advise.parse_argument.awk" \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/lineedit.awk"   \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/gsel.awk"       \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/lsel.awk"       \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/textbox.awk"    \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/statusline.awk" \
        -f "$___X_CMD_ROOT_MOD/tui/lib/comp/navi.awk"       \
        -f "$___X_CMD_ROOT_MOD/helpapp/lib/awk/help.renderer.awk"       \
        -f "$___X_CMD_ROOT_MOD/helpapp/lib/awk/app.awk"
}
