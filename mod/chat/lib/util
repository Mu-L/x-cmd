# shellcheck shell=dash

___x_cmd_chat_jqu(){
    local IFS=" "
    printf "%s" "$*" | command awk '
{
    text = (text == "") ? $0 : (text "\n" $0)
}
END{
    gsub(/\\/, "\\\\", text)
    gsub("\n", "\\n", text)
    gsub("\"", "\\\"", text)
    printf("\"%s\"", text)
}
'

}

___x_cmd_chat_help(){
    x help -m chat "$@" >&2
}