# shellcheck shell=dash
___x_cmd_chat_request_impl(){
    ___x_cmd_chat_request___apiv2 \
        ${___X_CMD_CHAT_CONFIRM_BEFORE_SEND:+"-c"} "$@"
}

___x_cmd_chat_request_impl_output(){
    local output="$1"
    if [ -t 1 ] && ___x_cmd_is_interactive; then
        case "$output" in
            normal)     x md show ;;
            json)       x jo color ;;
            *)          ___x_cmd_cmds_cat ;;
        esac
    else
        ___x_cmd_cmds_cat
    fi
}

