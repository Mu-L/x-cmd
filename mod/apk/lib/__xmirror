# shellcheck shell=dash
___x_cmd_apk___xmirror(){
    [ $# -gt 0 ]    ||  set - --help
    local op="$1";      shift

    local O=apk
    case "$op" in
        ls|set|unset|current)      x mirror apk               "$op" "$@" ;;  # In the future, we will put the mirror code here ...
        -h|--help)                 x help -m apk mirror             "$@" ;;
        *)                         ___x_cmd_apk_mirror___exec "$op" "$@" ;;
    esac
}


