# shellcheck shell=dash

xrc:mod:lib     sd      _cmd fzf/_index

___x_cmd_sd___main(){
    [ "$#" -gt 0 ] ||   set -- --fzfapp

    local op="$1"; shift
    case "$op" in
        --fzfapp|--fzfapppreview)
                    ___x_cmd_sd___"${op#--}"    "$@" ;;

        --cmd)      ___x_cmd_sd___cmd           "$@" ;;
        *)          ___x_cmd_sd___cmd "$op"     "$@" ;;
    esac
}

