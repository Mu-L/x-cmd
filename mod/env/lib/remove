# shellcheck    shell=dash
___x_cmd_env_remove(){
    local X_help_cmd='___x_cmd_env_help remove';   help:arg-null:parse

    local ___X_CMD_ENV___PARSE_PKG
    local ___X_CMD_ENV___PARSE_VERSION
    local i; for i in "$@"; do
        ___x_cmd_env___pasre_pkg_version_unset "$i"
        ___x_cmd_env_unuse "$___X_CMD_ENV___PARSE_PKG=$___X_CMD_ENV___PARSE_VERSION" || return
        ___x_cmd_pkg_remove "$___X_CMD_ENV___PARSE_PKG" "$___X_CMD_ENV___PARSE_VERSION" ||  return
    done
}
