{
    "ls": {
        "#desc": "list candidate version in use or available",
        "use|u": {
            "#desc":"list all in use candidate version",
            "#1": {
                "#exec": "___x_cmd_env___advise_ls_all_cand"
            }
        },
        "all|a": {
            "#desc":"list all available candidate version",
            "#1": {
                "#exec": "___x_cmd_env___advise_ls_all_cand"
            }
        }
    },
    "try": {
        "#desc":"Set environment version in this shell session",
        "#n":{
            "#exec": "___x_cmd_env___advise_ls_cand"
        }
    },
    "untry": {
        "#desc": "Deactivate and record environment version",
        "#1": {
            "#exec": "___x_cmd_env___advive_local_use_cand"
        },
        "#2": {
            "#exec": "___x_cmd_env___advive_local_use_version"
        }
    },
    "use": {
        "#desc": "Set global environment version",
        "#1": {
            "#exec": "___x_cmd_env___advise_ls_all_cand"
        },
        "#2":{
            "#exec": "___x_cmd_env___advise_ls_all_version"
        }
    },
    "unuse": {
        "#desc": "Remove environment",
        "#1": {
            "#exec": "___x_cmd_env___advive_local_use_cand"
        },
        "#2": {
            "#exec": "___x_cmd_env___advive_local_use_version"
        }
    },
    "exec": {
        "#desc": "Run an executable with the selected candidates version",
        "#1": {
            "#exec": "___x_cmd_env___advise_ls_installed_name"
        },
        "#2": {
            "#cand": []
        }
    },
    "which": {
        "#desc": "Display path of environment installed",
        "#1": {
            "#exec": "___x_cmd_env___advise_ls_installed_name"
        },
        "#2":{
            "#cand": [ ]
        }
    },
    "var": {
        "#desc": "Backup or recover environment",
        "#1": {
            "#cand": [ "backup", "get_backup", "ls", "recover" ]
        }
    },
    "prev": {
        "#desc": "Show the last version you used"
    },
    "current": {
        "#desc": "Input your local environment",
        "#1": {
            "#exec": "___x_cmd_env___advise_ls_installed_name"
        },
        "#2":{
            "#cand": [ ]
        }
    }
}