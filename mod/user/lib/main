# shellcheck shell=dash

xrc:mod:lib     user        add ls current

___x_cmd_user___main(){
    [ "$#" -gt 0 ] || {
        ___x_cmd_user_current
        return
    }

    local op="$1"; shift
    case "$op" in
        add|ls|rm|current)
            ___x_cmd_user_"$op" "$@" ;;
        -h|--help) x advise gen_help "$___X_CMD_ROOT_MOD/user/res/advise.jso" "$@" ;;
    esac
}
