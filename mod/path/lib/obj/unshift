# shellcheck shell=bash
___x_cmd_path_obj_unshift_(){
    local pathdata="${1}"
    local dir="${2:?Provide a directory}"

    if [ -z "$pathdata" ]; then         x_="${dir}"
    else                                x_="${dir}:${pathdata}"
    fi
}

___x_cmd_path_obj_unshift(){
    local x_
    ___x_cmd_path_obj_unshift_ "$@" || return
    printf "%s\n" "$x_"
}
